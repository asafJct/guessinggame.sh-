#!/usr/bin/env bash

echo "Welcome to guessing game"
echo "Please enter a guess number"

read guess

num = $(ls|wc -l)

function check{
if [[num -gt guess ]]
then
 echo "Your guess is too high"
else
 echo "Your guess is too low"
fi
}

while [[guess -ne num]]
do
 check
 read guess
done

echo "Conguratulations you have guess well"




